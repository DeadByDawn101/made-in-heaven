# Project Mariner — Environment Variables
# Copy to .env and fill in your values

# LLM API Keys (at least one required)
ANTHROPIC_API_KEY=your_anthropic_key_here
XAI_API_KEY=your_grok_key_here
OPENAI_API_KEY=your_openai_key_here

# Chrome DevTools connection
CHROME_URL=http://127.0.0.1:9222

# Path to chrome-devtools-mcp binary
# Default: auto-detected from PATH
MCP_BIN=

# Max steps per task (default: 30)
MAX_STEPS=30

# Task to run (alternative to --task CLI flag)
MARINER_TASK=

# Grok / xAI (for X-native mode)
XAI_API_KEY=your_xai_key_here

# Solana RPC — QuickNode (https://dashboard.quicknode.com/endpoints/new/SOL/solana-mainnet)
# Falls back to public mainnet if not set (rate-limited)
QUICKNODE_SOLANA_RPC=https://your-endpoint.quiknode.pro/your-key/
